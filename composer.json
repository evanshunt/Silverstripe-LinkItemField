{
    "name": "evanshunt/silverstripe-linkitemfield",
    "description": "SilverStripe 4 custom form element for anchor, email, telephone, file, image, internal and external links",
    "type": "silverstripe-vendormodule",
    "homepage": "http://github.com/evanshunt/silverstripe-linkitemfield",
    "keywords": [
        "silverstripe",
        "silverstripe 4",
        "linking",
        "custom link field"
    ],
    "license": "MIT",
    "support": {
        "issues": "http://github.com/evanshunt/silverstripe-linkitemfield/issues"
    },
    "require": {
        "php": ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "evanshunt\\LinkItemField\\": "src/",
            "evanshunt\\LinkItemField\\Tests\\": "tests/"
        }
    },
    "extra": {
        "installer-name": "linkitemfield",
        "expose": [
            "assets/css",
            "assets/js"
        ]
    }
}
